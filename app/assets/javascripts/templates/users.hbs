<div class="container-fluid">
  <div class="row-fluid">
    <div class="span3">
      <table class="table">
        <thead>
          <tr>
            <th>Users</th>
          </tr>
        </thead>
        <tbody>
          {{#each model}}
          <tr>
            <td>
              {{name}}<small class"muted"> by {{email}}</small>
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    <div class="span9">
      {{outlet}}
    </div>
  </div>
</div>
